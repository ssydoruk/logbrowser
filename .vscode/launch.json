    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Start<logbrowser>",
            "request": "launch",
            "mainClass": "com.myutils.logbrowser.common.Start",
            "projectName": "logbrowser"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Main<logbrowser>",
            "request": "launch",
            "mainClass": "com.myutils.logbrowser.indexer.Main",
            "projectName": "logbrowser"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Parser<logbrowser>",
            "request": "launch",
            "mainClass": "com.myutils.logbrowser.indexer.Parser",
            "projectName": "logbrowser"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-inquirer<logbrowser>",
            "request": "launch",
            "mainClass": "com.myutils.logbrowser.inquirer.inquirer",
            "projectName": "logbrowser"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-GridBagLayoutDemo",
            "request": "launch",
            "mainClass": "com.airbnb.utils.GridBagLayoutDemo"
        }
    ]
}